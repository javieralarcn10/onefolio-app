{
  "expo": {
    "name": "Onefolio",
    "slug": "onefolio",
    "version": "1.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "onefolio",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.javieralarcn10.onefolio",
      "usesAppleSignIn": true,
      "appleTeamId": "TTNJQVH4P3",
      "infoPlist": {
        "GIDClientID": "772253634590-f9oe48o7uded4a1ui5s3701hmr0q8cjn.apps.googleusercontent.com",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.javieralarcn10.onefolio",
      "permissions": [
        "android.permission.USE_BIOMETRIC",
        "android.permission.USE_FINGERPRINT",
        "android.permission.USE_BIOMETRIC",
        "android.permission.USE_FINGERPRINT"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "./plugins/with-ios-assets",
      [
        "expo-local-authentication",
        {
          "faceIDPermission": "Allow $(PRODUCT_NAME) to use Face ID."
        }
      ],
      [
        "expo-notifications",
        {
          "enableBackgroundRemoteNotifications": true
        }
      ],
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/icon.png",
          "imageWidth": 150,
          "resizeMode": "contain",
          "backgroundColor": "#191919",
          "dark": {
            "backgroundColor": "#191919"
          }
        }
      ],
      [
        "expo-apple-authentication"
      ],
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/Lausanne-300.otf",
            "./assets/fonts/Lausanne-400.otf",
            "./assets/fonts/Lausanne-500.otf",
            "./assets/fonts/Lausanne-600.otf",
            "./assets/fonts/Lausanne-700.otf",
            "./assets/fonts/Lausanne-800.otf"
          ]
        }
      ],
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps.772253634590-f9oe48o7uded4a1ui5s3701hmr0q8cjn"
        }
      ],
      "expo-localization",
      "@react-native-community/datetimepicker"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "00fd6b65-6519-4194-a964-0a371b916d87"
      }
    },
    "owner": "javieralarcn10",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/00fd6b65-6519-4194-a964-0a371b916d87"
    }
  }
}
